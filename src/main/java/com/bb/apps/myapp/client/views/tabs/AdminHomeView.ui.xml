	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	
	<ui:with type="com.bb.apps.myapp.client.IMyAppResources" field="res"></ui:with>

	<g:HTMLPanel width="100%" height="350px" styleName="{res.myAppStyles.whiteCanvas}">
		
